@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="text-center">
    <br>
    <br>
    <br>
    <h1 class="display-4">Bienvenido a Asignacion de Salones</h1>
    <p>Reserva ya tu salón!!!</p>   

</div>

@if (User.Identity.IsAuthenticated)
{
    <table class="table table-sm">
        @foreach(var claim in User.Claims) 
        {
            <tr>
                <td>
                    @claim.Type
                </td>
                <td>
                    @claim.Value
                </td>
            </tr>
        }      
    </table>
}

